    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.21",
    "@types/nodemailer": "^6.4.0",
    "@types/qs": "^6.9.0",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "aos": "^2.3.4",
    "axios": "^0.19.0",
    "brotli-webpack-plugin": "^1.1.0",
