    "next": "^9.1.6",
    "next-compose-plugins": "^2.2.0",
    "next-env": "^1.1.0",
    "next-fonts": "^0.19.0",
    "next-images": "^1.2.0",
    "next-offline": "^4.0.6",
    "next-seo": "^3.2.0",
    "node-sass": "^4.13.0",
    "nodemailer": "^6.4.2",
