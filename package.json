  "scripts": {
    "dev": "next dev",
    "start": "cross-env NODE_ENV=production next start",
    "build": "cross-env NODE_ENV=production next build",
    "export-only": "cross-env NODE_ENV=production next export",
    "export": "run-s build export-only",
    "analyze": "cross-env ANALYZE=true next build",
    "run-p": "run-p",
    "run-s": "run-s",
    "test": "jest --passWithNoTests --all",
    "test-live": "jest --passWithNoTests --all --watch",
    "lint": "prettier ./{*,public/**/*,src/**/*}.{ts,tsx,js,jsx,css,less,scss,json,md,html}",
    "format": "prettier --write ./{*,public/**/*,src/**/*}.{ts,tsx,js,jsx,css,less,scss,json,md,html}",
    "prod": "run-p format build test",
    "UNSAFE_upgrade": "yarn add @fortawesome/fontawesome-svg-core @fortawesome/free-brands-svg-icons @fortawesome/free-regular-svg-icons @fortawesome/free-solid-svg-icons @fortawesome/react-fontawesome @next/bundle-analyzer @types/aos @types/jest @types/node @types/nodemailer @types/qs @types/react @types/react-dom @zeit/next-css @zeit/next-sass aos axios brotli-webpack-plugin compression-webpack-plugin cross-env dotenv-load gerami husky imagemin-gifsicle imagemin-mozjpeg imagemin-pngquant img-loader isomorphic-unfetch jest next next-compose-plugins next-env next-fonts next-images next-offline next-seo node-sass nodemailer npm-run-all optimize-css-assets-webpack-plugin prettier qs react react-dom react-ga react-parallax react-router-dom typescript webpack --latest"
  },
